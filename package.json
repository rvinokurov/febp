{
  "name": "febp",
  "version": "0.0.0-PLACEHOLDER",
  "description": "Frontend boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/iboozyvoozy/febp.git"
  },
  "scripts": {
    "postinstall": "webdriver-manager update",
    "start": "npm run build",
    "test": "npm-run-all test:unit coverage:*",
    "test:e2e": "protractor config/protractor.prod.js",
    "test:e2e:dev": "protractor config/protractor.dev.js",
    "validate": "npm-run-all --parallel validate:* lint",
    "validate:dev": "webpack-validator config/webpack.dev.js",
    "validate:test": "webpack-validator config/webpack.test.js",
    "validate:prod": "webpack-validator config/webpack.prod.js",
    "lint": "eslint config",
    "serve": "node server.js",
    "serve:dev": "browser-sync start --config config/browser-sync.js",
    "serve:mocks": "json-server mocks/api.js --config mocks/config.json",
    "serve:webdriver": "webdriver-manager start",
    "build": "webpack --config config/webpack.prod.js",
    "build:dev": "webpack --config config/webpack.dev.js",
    "test:unit": "karma start config/karma.prod.js",
    "test:unit:dev": "karma start config/karma.dev.js",
    "coverage:check": "istanbul check-coverage",
    "coverage:report": "cat ./reports/coverage/lcov.info | codeclimate-test-reporter",
    "commit": "git-cz",
    "release": "semantic-release pre && npm publish && semantic-release post"
  },
  "dependencies": {
    "angular": "1.5.8",
    "angular-mocks": "1.5.8",
    "autoprefixer": "6.4.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.14.0",
    "clean-webpack-plugin": "0.1.10",
    "codeclimate-test-reporter": "0.3.3",
    "commitizen": "2.8.6",
    "compression": "1.6.2",
    "css-loader": "0.24.0",
    "cz-conventional-changelog": "1.2.0",
    "debug": "2.2.0",
    "dotenv": "2.0.0",
    "eslint": "3.4.0",
    "eslint-loader": "1.5.0",
    "exports-loader": "0.6.3",
    "express": "4.14.0",
    "express-history-api-fallback": "2.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "imports-loader": "0.6.5",
    "jade": "1.11.0",
    "jade-html-loader": "0.0.3",
    "lodash": "4.15.0",
    "ng-annotate-loader": "0.1.1",
    "ng-cache-loader": "0.0.20",
    "npm-run-all": "3.0.0",
    "postcss-loader": "0.11.1",
    "pug": "2.0.0-beta4",
    "pug-loader": "2.3.0",
    "require-environment-variables": "1.1.2",
    "semantic-release": "4.3.5",
    "style-loader": "0.13.1",
    "stylint": "1.5.6",
    "stylint-loader": "1.0.0",
    "stylus": "0.54.5",
    "stylus-loader": "2.3.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-merge": "0.14.1",
    "webpack-validator": "2.2.7"
  },
  "devDependencies": {
    "babel-plugin-__coverage__": "11.0.0",
    "babel-register": "6.14.0",
    "browser-sync": "2.14.3",
    "coveralls": "2.11.12",
    "istanbul": "0.4.5",
    "jasmine": "2.5.0",
    "json-server": "0.8.19",
    "karma": "1.2.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-html-reporter": "0.2.7",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "2.1.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-webpack": "1.8.0",
    "protractor": "4.0.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  },
  "keywords": [
    "frontend",
    "angular",
    "webpack",
    "babel"
  ]
}
